;DIMENSION_UNIT=1
VARS
   DX := 24;*VAR*Dimension X
   DY := 18;*VAR*Dimension Y
   DZ := 0.75;*VAR*Dimension Z
   _T2CD := 0;
   _T3CD := 0;
   _T4CD := 0;
   _T7CD := 0;
   _T9CD := 0;
   CD := 0;
   TOOL_NUMBER := 3;*VAR*2.3.4.7.9
   _KD_X := 0;
   _KD_Y := 0;
   _KD_Z := 0;
   RADIUS := 0;*VAR*
   SQUARE_X := 5;*VAR*
   SQUARE_Y := 5;*VAR*
START
Fertigteil (DX,DY,DZ,0,0,0,0,0,'STARTING POINT PROGRAM',0,0,0,0)
CALL Park_V7 ( VAL MODE:=2,POSX:=0,POSY:=0)
;IMPORT ALL GLOBAL CD VARS
ReadVar (_T2CD)
ReadVar (_T3CD)
ReadVar (_T4CD)
ReadVar (_T7CD)
ReadVar (_T9CD)
IF TOOL_NUMBER = 2
CD := _T2CD
WZF (2,_VE*0.5,_V*0.5,_VA*0.5,_SD,_ANF,'1')
END
IF TOOL_NUMBER = 3
CD := _T3CD
WZF (3,_VE*0.5,_V*0.5,_VA*0.5,_SD,_ANF,'1')
END
IF TOOL_NUMBER = 4
CD := _T4CD
WZF (4,_VE*0.5,_V*0.5,_VA*0.5,_SD,_ANF,'1')
END
IF TOOL_NUMBER = 7
CD := _T7CD
WZF (7,_VE*0.5,_V*0.5,_VA*0.5,_SD,_ANF,'1')
END
IF TOOL_NUMBER = 9
CD := _T9CD
WZF (9,_VE*0.5,_V*0.5,_VA*0.5,_SD,_ANF,'1')
END
;END OF GLOBAL IMPORTS AND SETTING CUT DEPTH
IF RADIUS <> 0
CALL _Kreisbahn_V5 ( VAL X_MITTE:=0,Y_MITTE:=0,TIEFE:=CD,ZUTIEFE:=_AT_MAXDEPTH,RADIUS:=RADIUS,RADIUSKORREKTUR:=1,AB:=1,AUFMASS:=0,BEARB_UMKEHREN:=0,WINKEL:=180,ANF:=_ANF,ABF:=_ANF,RAMPE:=1,INTERPOL:=1,ESXY:=9,ESMD:=1,LASER:=0)
END
IF (SQUARE_X <> 0) & (SQUARE_Y <>0)
CALL _Rechteck_V5 ( VAL X_MITTE:=0,Y_MITTE:=0,LAENGE:=SQUARE_X,BREITE:=SQUARE_Y,RADIUS:=0,WINKEL:=0,TIEFE:=CD,ZUTIEFE:=0,RADIUSKORREKTUR:=2,AB:=2,AUFMASS:=0,ANF:=_ANF,ABF:=_ANF,UMKEHREN:=0,RAMPE:=0,RAMPENLAENGE:=50,QUADRANT:=1,INTERPOL:=0,ESXY:=9,ESMD:=1,LASER:=0)
END
